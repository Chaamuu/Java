abstract class animal{
    public abstract void sound();
}
class Dog extends animal{
    public void sound(){
        System.out.println("dog is barking");
    }
}
class Lion extends animal{
    public void sound(){
        System.out.println("lion is roaring");
    }
}
public class test{
    public static void main(String[] args){
        Dog d=new Dog();
        Lion l=new Lion();
        d.sound(); l.sound();
    }
}
