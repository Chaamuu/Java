import java.util.*;
public class Hashing{
    public static void main(String[] args){
        HashMap<String,Integer> map=new HashMap<>();
        
        //insertion
        map.put("india",430);
        map.put("pakistan",321);
        map.put("china",120);
    
    System.out.println(map);
    
    map.put("china",180);
    System.out.println(map);//lookup operation
    
    //search
    if(map.containsKey("china")){
    System.out.println("key is present in the map");
    }
    else{
        System.out.println("key is not present in the map");
    }
    System.out.println(map.get("china"));//key exsist
    System.out.println(map.get("kerala"));//key does'nt exsist
}
}
